{
    "name": "softspring/media-bundle",
    "description": "An media management bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/migrations": "^3.2",
        "doctrine/orm": "^2.12",
        "google/cloud-storage": "^1.20",
        "imagine/imagine": "^1.3",
        "softspring/crudl-controller": "^5.2",
        "softspring/doctrine-paginator": "^5.2",
        "softspring/doctrine-query-filters": "^5.2",
        "softspring/doctrine-target-entity-resolver": "^5.2",
        "softspring/events": "^5.2",
        "softspring/permissions-bundle": "^5.2",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "symfony/routing": "^5.4|^6.0|^7.0",
        "symfony/translation": "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0",
        "twig/twig": "^2.10|^3.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5.2"
    },
    "suggest": {
        "softspring/crudl-controller": "Required if you want to enable admin controllers",
        "google/cloud-storage": "Required to integrate media storage in GCS"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\MediaBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
