{
    "name": "softspring/media-bundle",
    "description": "An media management bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-gd": "*",
        "imagine/imagine": "^1.2",
        "softspring/core-bundle": "^5.0",
        "softspring/crudl-controller": "^5.0",
        "softspring/events": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "google/cloud-storage": "^1.20",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "doctrine/orm": "^2.12",
        "phpunit/phpunit": "^9.5",
        "symfony/form": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "symfony/routing": "^5.4|^6.0",
        "twig/twig": "^3.4",
        "symfony/translation": "^5.4|^6.0"
    },
    "suggest": {
        "softspring/crudl-controller": "Required if you want to enable admin controllers",
        "google/cloud-storage": "Required to integrate media storage in GCS"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\MediaBundle\\Tests\\": "tests/"
        }
    }
}
